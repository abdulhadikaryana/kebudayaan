tle: {                    text: data.title,                    x: -20 //center                },                subtitle: {                    text: '',                    x: -20 //center                },                xAxis: {                    title: {                        text: data.xLabel                    },                    categories: data.category                },                yAxis: {                    title: {                        text: data.yLabel                    },                    plotLines: [{                        value: 0,                        width: 1,                        color: '#808080'                    }]                },                legend: {                    enabled: false                },                tooltip: {                    formatter: function() {                        return this.x +': '+ this.y;                    }                },                plotOptions: {                    series: {                        cursor: 'pointer',                        point: {                            events: {                                click: function(e) {                                    switch(this.config[0])                                    {                                        case 'cat':                                            subCategoryClick(this.config[2], '/admin/index/subcategory/', 1);                                            e.preventDefault();                                            break;                                        case 'area':                                            subCategoryClick(this.config[2], '/admin/index/subarea/', 2);                                            e.preventDefault();                                            break;                                        case 'pie':                                            pieChart(this.config[2]);                                            e.preventDefault();                                            break;                                    }                                }                            }                        }                    }                },                series: [{                    name: '',                    data: data.total                }]            });    }    function pie_chart(data)    {        var chart = new Highcharts.Chart({            chart: {                renderTo: 'pie-chart',                plotBackgroundColor: null,                plotBorderWidth: null,                plotShadow: false            },            title: {                text: data.title            },            tooltip: {                pointFormat: '{series.name}: <b>{point.percentage}%</b>',                percentageDecimals: 1            },            plotOptions: {                pie: {                    allowPointSelect: true,                    cursor: 'pointer',                    dataLabels: {                        enabled: true,                        color: '#000000',                        connectorColor: '#000000',                        formatter: function() {                            return '<b>'+ this.point.config[0] +'</b>: '+ this.point.config[1] +' %';                        }                    }                }            },            series: [{                type: 'pie',                name: 'Browser share',                data: data.total            }]        });    }</script>